{
  "welcomeFile": "admin.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [{
		"source": "^/admin/(.*)$",
		"target": "/admin/$1",
		"destination": "SRV_API_URL",
		"authenticationType": "xsuaa"
	}, {
		"source": "^(.*)$",
		"target": "$1",
    "localDir": "./",
		"authenticationType": "xsuaa"
	}]
}
