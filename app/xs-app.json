{
  "welcomeFile": "admin.html",
  "authenticationMethod": "route",
  "websockets": {
    "enabled": true
  },
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/admin/(.*)$",
      "target": "/admin/$1",
      "destination": "SRV_API_URL",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/catalog/(.*)$",
      "target": "/catalog/$1",
      "destination": "SRV_API_URL",
      "authenticationType": "none"
    },
    {
      "source": "^/wss$",
      "target": "/wss",
      "destination": "SRV_API_URL",
      "authenticationType": "none"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "localDir": "./",
      "authenticationType": "xsuaa"
    }
  ]
}
